class Solution {
public:
    int maxProfit(vector<int>& prices) {
                
        int n = prices.size();
        
        int ans = 0;
        
        int maxEl = prices[n - 1];
        
        for(int i = n - 2; i >= 0;i--){
            ans = max(ans , maxEl - prices[i]);
            
            if(maxEl < prices[i])maxEl = prices[i];
        }
        return ans;
    }
};